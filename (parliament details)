      "cell_type": "code",
      "source": [
        "column_data =soup.find_all('tr')"
      ],
      "metadata": {
        "id": "69ELa4tCBc8S"
      },
      "execution_count": 18,
      "outputs": []
    },
    {
      "cell_type": "code",
      "source": [
        "# prompt: generate the whole table in the given url page with all the 11 pages\n",
        "\n",
        "import requests\n",
        "from bs4 import BeautifulSoup\n",
        "import pandas as pd\n",
        "\n",
        "column_names = []\n",
        "all_rows = []\n",
        "\n",
        "for page_num in range(1, 12):  # Iterate through pages 1 to 11\n",
        "    url = f'https://www.mpa.gov.in/unit-telephone/{page_num}000'\n",
        "    response = requests.get(url)\n",
        "    soup = BeautifulSoup(response.content, 'html.parser')\n",
        "\n",
        "    if page_num == 1:  # Extract column names only from the first page\n",
        "        table_header = soup.find('table')\n",
        "        column_names = [th.text.strip() for th in table_header.find_all('th')]\n",
        "\n",
        "    rows = soup.find_all('tr')[1:]  # Skip header row\n",
        "    for row in rows:\n",
        "        row_data = [td.text.strip() for td in row.find_all('td')]\n",
        "        all_rows.append(row_data)\n",
        "\n",
        "df = pd.DataFrame(all_rows, columns=column_names)\n",
        "print(df)\n"
      ],
